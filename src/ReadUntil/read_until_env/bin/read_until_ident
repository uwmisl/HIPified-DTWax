#!/home/hariss.not/DTWax/src/ReadUntil/read_until_env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'read-until==2.1.0','console_scripts','read_until_ident'
__requires__ = 'read-until==2.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('read-until==2.1.0', 'console_scripts', 'read_until_ident')()
    )
